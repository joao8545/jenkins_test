node {
    stage('Build') {
        echo 'Building....'
    }
    stage('Test') {
        echo 'Building....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
    stage('Parallel') {
        parallel firstBranch: {
            sleep time: 300, unit: 'MILLISECONDS'
            echo 'first branch'
        }, secondBranch: {
            sleep time: 200, unit: 'MILLISECONDS'
            echo 'second branch'
        }, thirdBranch: {
            sleep time: 400, unit: 'MILLISECONDS'
            echo 'third branch'
        },fourthBranch: {
            sleep time: 100, unit: 'MILLISECONDS'
            echo 'fourth branch'
        } 
    }
}
