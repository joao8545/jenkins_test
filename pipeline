pipeline{
    agent any
    stages{
        stage('stage1') {
            steps{       
                echo 'hello world!!!'
            }
        }
        stage('stage2') {
            steps{       
                echo 'hello world!2'
            }
        }
        stage('stage3') {
            steps{       
                echo 'hello world3 '
            }
        }
    }
}